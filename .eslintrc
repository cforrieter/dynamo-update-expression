{
  "parserOptions": {
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": [],
  "extends": ["eslint:recommended"],
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "rules": {
    "class-methods-use-this": [0],
    "no-unused-vars": [
      "error",
      { "vars": "all", "args": "after-used", "varsIgnorePattern": "^_", "ignoreRestSiblings": false }
    ]
  }
}
